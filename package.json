{
	"name": "@ctx-core/fast-deep-equal",
	"version": "1.0.16",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@"
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.121",
		"@types/node": "^17.0.1",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"noUpdate": [],
	"homepage": "https://github.com/ctx-core/fast-deep-equal#readme",
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fast-deep-equal.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/fast-deep-equal/issues"
	}
}
